    #Sece Dining
    a=input()
b=int(input())
if(a=="front"):
    if(b==1):
        print("Left Handed")
    elif(b==2):
        print("Right Handed")
    else:
        print("invalid input")
elif(a=="rear"):
    if(b==1):
        print("Right Handed")
    elif(b==2):
        print("Left Handed")
    else:
        print("invalid input")
else:
    print("invalid input")
    
    
    
    #Intersection of a Circle
    x1=int(input())
y1=int(input())
r1=int(input())
x2=int(input())
y2=int(input())
r2=int(input())
c1c2=((x1-x2)**2+(y1-y2)**2)**0.5
if(c1c2==r1+r2):
    print("The circles are tangential")
elif(c1c2<r1+r2):
    print("The circles overlap")
else:
    print("The circles do not overlap")
    
    
    #Microwave Oven
    a=int(input("Enter the number of items:"))
b=float(input("Enter the single item heating time:"))
x=b+((50/100)*b)
y=b+b
if(a==1):
    print("The recommended heating time is %.2f"%a)
elif(a==2):
    print("The recommended heating time is %.2f"%x)
elif(a==3):
    print("The recommended heating time is %.2f"%y)
else:
    print("Number of items is more")
