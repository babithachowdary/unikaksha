    #Y2K Problem Detector
    print("Enter year of birth")
by=int(input())
print("Enter current year")
cy=int(input())
if by>cy:
    B=100-by
    A=B+cy
    print("your age is ",A)
else:
        a=cy-by
        print("your age is ",a)
        
        
        #Lab Allocation
        print("enter x")
x=int(input())
print("enter y")
y=int(input())
print("enter z")
z=int(input())
if x<y and x<z:
    print("L1 has the minimal seating capacity")
elif y<x and y<z:
    print("L2 has the minimal seating capacity")
elif z<x and z<y:#python
    print("L3 has the minimal seating capacity")
else:
    print("invalid input")
    
    
    #Gardening I
    r=int(input())
c=int(input())
n=int(input())
if n%r==0:
    print("yes")
elif n%c==1:
    print("yes")
else:
    print("no")
    
    
  #Hotel Tariff Calculator
  month=int(input())
rent=float(input())
days=int(input())
a=rent*days
x=rent*(20/100)
y=(rent+x)*days
if month<=3 or month>=7 and month<=10:
    print("Hotel Tariff:Rs.%.2f"%a)
elif month>=4 and month<=6 or month>=11 and month<=12:
    print("Hotel Tariff:Rs.%.2f"%y)
else:
    print("invalid input")
    
    
