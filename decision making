#Greatest of two numbers
a=int(input())
b=int(input())
if(a==b):
    print(a,"is equal to",b)
elif(a<b):
    print(a,"is less than",b)
else:
    print(a,"is greater than",b)


#Vowel Or Consonants
s=input()
if s in"AEIOUaeiou":
    print("vowel")
elif s in"BCDFGHIJKLMNPQRSTVWXYZbcdfghijklmnpqrstvwxyz":
    print("consonant")
else:
    print("not an alphabet")
    
     
#Grading System
marks=int(input())
if marks==100:
    print("grade-S")
if marks>90 and marks<100:
    print("grade-A")
elif marks>80 and marks<=90:
    print("grade-B")
elif marks>70 and marks<=80:
    print("grade-C")
elif marks>60 and marks<=70:
    print("grade-D")
elif marks>50 and marks<=60:
    print("grade-E")
elif marks<50:
    print("grade-F")
elif marks>100:
    print("invalid input")
else:
    print("fail")
    
    
    #Profit or Loss
    cp=float(input())
spofonemango=int(input())
sp=spofonemango*12
print("Enter the price of a dozen mangoes")
print("Enter the price at which 1 mango is being sold")
if cp==sp:
  print("No profit nor loss")
elif sp>cp:
  print("profit:Rs.%.2f"%(sp-cp))
else:
  print("Loss:Rs.%.2f"%(cp-sp))
  
  
